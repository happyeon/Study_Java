package com.java05_class;

// class : 설계도
// instance : 객체 - class를 가지고 memory에 실제로 구현된 구현체
public class Animal {
	
	// field : 속성
	private String kind;
	private String name;
	private int age;
	
	// 기본 생성자
	public Animal() {
		
	}
	
	// 파라미너 있는 생성자
	public Animal(String kind, String name, int age) {
		this.kind = kind;
		this.name = name;
		this.age = age;
	}
}
